/*上拉触底样式 开始  */

/*  加载更多   */

.load_box {
    background: #fff;
    width: 100%;
}

.center {
    display: flex;
    align-items: center;
    justify-content: center;
}

.weui-loading {
    margin: 0 5px;
    width: 20px;
    height: 20px;
    display: inline-block;
    vertical-align: middle;
    -webkit-animation: weuiLoading 1s steps(12, end) infinite;
    animation: weuiLoading 1s steps(12, end) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
}

/*上拉触底样式 开始  */

.weui-loadmore {
    padding: 1.5em 0;
     background: #fff;
    width: 65%;
    margin:0 auto;
    line-height: 1.6em;
    font-size: 14px;
    text-align: center;
}

.weui-loadmore__tips {
    display: inline-block;
    vertical-align: middle;
}

/*上拉触底样式 结束  */
