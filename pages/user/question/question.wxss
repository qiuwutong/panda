/* pages/user/question/question.wxss */
.content {
    background: #fff;
    font-size: 28rpx;
}
.content .top {
    height: 88rpx;
    background: #f1f1f1;
    padding-left: 20rpx;
    line-height: 88rpx;
    color: #969696;
}
.content .info .lists {
    display: flex;
    flex-direction: column;
}
.content .info .lists .list{
    height: 98rpx;
    padding:0 20rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1rpx solid #f1f1f1;
}
.content .info .lists .list .iconfont {
    color: #969696;
    font-weight: 900;
}